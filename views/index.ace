= doctype html
html lang=en
  head
    meta charset=utf-8
    title Hub
  body
    h1 Welcome to Hub!
    a href="/login" Login

    h5 {{.Title}}
    ul
      {{range .Msgs}}
        li {{.}}
      {{end}}

    h5 {{.Map.ceva}}

    {{if .User}}
      h2 Hello, {{.User}}
    {{else}}
      h2 You are not logged in.
    {{end}}